def generate_fibonacci(n):
    fib_sequence = []
    a, b = 0, 1
    
    for _ in range(n):
        fib_sequence.append(a)
        a, b = b, a + b
    
    return fib_sequence

# Change 'n' to the number of Fibonacci terms you want to generate
n = 10
fib_sequence = generate_fibonacci(n)
print(f"The first {n} Fibonacci numbers are: {fib_sequence}")
